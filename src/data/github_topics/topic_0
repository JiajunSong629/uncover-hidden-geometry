    var app = angular.module('App', []);

    app
      .controller('MyCtrl', function($scope){
        var vm = $scope;

        vm.model = {
          setting: {
            honors: [
              {
                name: 'AllStar',
                value: 0
              },{
                name: 'Champion',
                value: 1
              },{
                name: 'FMVP',
                value: 2
              }
            ]
          },
          postData: {
            // honors: null,
            honors: [
              {value: false},
              {value: true},
              {value: true}
            ]
          }
        };
      });
  </script>

